{
  "parser": "@typescript-eslint/parser",
  "extends": ["plugin:@typescript-eslint/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-restricted-globals": ["error", "event", "name", "location"]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
